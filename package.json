{
  "name": "view",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vuedx-typecheck . && vite build",
    "lint": "eslint --fix --ext .js{x},.ts{x},.vue ./src",
    "test": "jest"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "axios": "^0.21.1",
    "echarts": "^5.0.1",
    "element-plus": "^1.0.2-beta.31",
    "lodash": "^4.17.20",
    "normalize.css": "^8.0.1",
    "vue": "^3.0.5",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0-rc.2"
  },
  "devDependencies": {
    "@ls-lint/ls-lint": "^1.9.2",
    "@types/jest": "^26.0.14",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "@vitejs/plugin-vue": "^1.1.2",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vuedx/typecheck": "^0.6.0",
    "@vuedx/typescript-plugin-vue": "^0.6.0",
    "chalk": "^4.1.0",
    "eslint": "^7.19.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.5.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "sass": "^1.32.5",
    "sass-loader": "^10.1.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "vite": "^2.0.0-beta.50"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 100,
    "semi": true,
    "arrowParens": "avoid"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": "eslint --fix",
    "*.{css,html,scss,json}": "prettier -w"
  },
  "husky": {
    "hooks": {
      "pre-commit": "ls-lint & lint-staged",
      "commit-msg": "node scripts/verify-commit.js"
    },
    "browserslist": [
      "> 1%",
      "last 2 versions",
      "not dead"
    ]
  }
}
